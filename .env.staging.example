# Staging Environment Configuration
# Copy to .env.staging and fill in actual values

# PostgreSQL
POSTGRES_USER=fib_staging
POSTGRES_PASSWORD=<CHANGE_ME>
POSTGRES_DB=fib_staging
PGUSER=fib_staging
PGHOST=<RDS_ENDPOINT>  # e.g., fib-staging.xxxxx.us-east-1.rds.amazonaws.com
PGDATABASE=fib_staging
PGPASSWORD=<CHANGE_ME>
PGPORT=5432

# Redis
REDIS_HOST=<ELASTICACHE_ENDPOINT>  # e.g., fib-staging.xxxxx.cache.amazonaws.com
REDIS_PORT=6379
