# Production Environment Configuration
# Copy to .env.production and fill in actual values

# PostgreSQL
POSTGRES_USER=fib_prod
POSTGRES_PASSWORD=<CHANGE_ME_STRONG_PASSWORD>
POSTGRES_DB=fib_production
PGUSER=fib_prod
PGHOST=<RDS_ENDPOINT>  # e.g., fib-prod.xxxxx.us-east-1.rds.amazonaws.com
PGDATABASE=fib_production
PGPASSWORD=<CHANGE_ME_STRONG_PASSWORD>
PGPORT=5432

# Redis
REDIS_HOST=<ELASTICACHE_ENDPOINT>  # e.g., fib-prod.xxxxx.cache.amazonaws.com
REDIS_PORT=6379
